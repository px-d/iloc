{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "enables the default permissions",
	"windows": ["main"],
	"permissions": [
		"core:default",
		"fs:default",
		{
			"identifier": "fs:default",
			"allow": [{ "path": "**" }]
		},
		{
			"identifier": "fs:write-all",
			"allow": [{ "path": "**" }]
		},
		{
			"identifier": "fs:allow-watch",
			"allow": [{ "path": "**" }]
		}
	]
}
